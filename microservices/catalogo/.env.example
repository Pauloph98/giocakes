# Variáveis de Ambiente - Serviço de Catálogo

# Configuração do Banco de Dados PostgreSQL
DATABASE_URL=postgresql://catalogo_user:catalogo_pass@localhost:5432/catalogo_db

# Porta do serviço
PORT=5001

# Modo debug (True para desenvolvimento, False para produção)
DEBUG=True

# Secret Key (gerar nova em produção)
SECRET_KEY=dev-secret-key-change-in-production

# CORS (permitir origens)
CORS_ORIGINS=http://localhost:8000,http://localhost:3000
